{
	"name": "Jenkins Notifier",
	"description": "Jenkins Notifier",
	"manifest_version" : 2,
	"version" : "0.1.0",
	"icons": {
		"128": "images/icon.png",
		"48": "images/icon.png",
		"16": "images/icon.png"
	},  
    "web_accessible_resources": [
	    "images/blue.png",
	    "images/red.png",
	    "images/yellow.png",
	    "images/grey.png"
    ],
    "options_page": "options.html",
    "background": {
        "page" : "background.html"
    },
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_title": "Jenkins Notifier",
        "default_popup": "options.html"
    },
    "permissions": [
        "background",
        "notifications",
        "http://*/",
        "https://*/"
    ]
}
